
import java.util.Scanner;
public class PositiveNegative{
    public static void main(String[] args){
        // Dylan Gabriel Santos
       Scanner s = new Scanner(System.in);
       int number;
       System.out.print("Enter a number to be identified:   ");
       number = s.nextInt();
       

       if( number > 0){
        System.out.print("Then it is positive");
       }else{
        System.out.print("Then it is negative");
       }



    }
}
